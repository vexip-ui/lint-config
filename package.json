{
  "name": "lint-config",
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "license": "MIT",
  "private": true,
  "author": "qmhc",
  "scripts": {
    "build": "pnpm -r run build",
    "lint": "eslint --fix .",
    "prettier": "prettier --write \"**/*.{js,cjs,ts,json,md}\"",
    "release": "tsx scripts/release.ts",
    "publish:ci": "tsx scripts/publish.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/types": "^19.0.3",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.16.5",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.7.0",
    "@vexip-ui/scripts": "^1.4.1",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^9.31.0",
    "husky": "^9.0.1",
    "is-ci": "^3.0.1",
    "jiti": "^2.4.2",
    "lint-staged": "^15.2.0",
    "minimist": "^1.2.8",
    "prettier": "^3.2.4",
    "prompts": "^2.4.2",
    "semver": "^7.7.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "stylelint",
        "@csstools/*"
      ]
    }
  }
}
